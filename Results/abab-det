digraph finite_state_machine {
	node [shape=doublecircle]
	node [shape=circle]
	2 -> 1 [label=a]
	2 -> 2 [label=b]
	21 -> 1 [label=a]
	21 -> 2 [label=b]
	0 -> 21 [label=b]
	1 -> 1 [label=a]
	1 -> 2 [label=b]
	2 -> 3 [label=a]
	3 -> 1 [label=a]
	2 -> 0 [label=b]
	3 -> 4 [label=b]
	4 -> 4 [label=a]
	4 -> 4 [label=b]
	2 -> 3 [label=a]
	2 -> 0 [label=b]
	21 -> 3 [label=a]
	21 -> 0 [label=b]
	0 -> 21 [label=b]
}

digraph finite_state_machine {
	node [shape=egg]
	0
	node [shape=doublecircle]
	3
	4
	1
	1
	3
	4
	01
	013
	123
	node [shape=circle]
	0 -> 2 [label=b]
	1 -> 1 [label=a]
	1 -> 3 [label=a]
	2 -> 4 [label=b]
	3 -> 1 [label=a]
	01 -> 2 [label=b]
	0 -> 01 [label=a]
	013 -> 2 [label=b]
	01 -> 013 [label=a]
	123 -> 4 [label=b]
	2 -> 123 [label=a]
	013 -> 013 [label=a]
	123 -> 123 [label=a]
}

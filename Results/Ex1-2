digraph finite_state_machine {
	node [shape=egg]
	0
	node [shape=doublecircle]
	4
	1
	node [shape=circle]
	0 -> 3 [label=a]
	0 -> 4 [label=a]
	0 -> 1 [label=b]
	0 -> 3 [label=c]
	1 -> 3 [label=a]
	1 -> 2 [label=b]
	1 -> 0 [label=c]
	1 -> 1 [label=c]
	2 -> 2 [label=a]
	2 -> 1 [label=b]
	2 -> 2 [label=c]
	2 -> 3 [label=c]
	3 -> 0 [label=a]
	3 -> 3 [label=b]
}

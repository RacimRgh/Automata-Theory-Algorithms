digraph finite_state_machine {
	node [shape=egg]
	"1,123"
	node [shape=doublecircle]
	"1,123"
	3
	4
	1
	node [shape=circle]
	0 -> 2 [label=b]
	1 -> 1 [label=a]
	1 -> 3 [label=a]
	2 -> 4 [label=b]
	3 -> 1 [label=a]
	3 -> 2 [label=b]
	0 -> 2 [label=a]
	"1,123" -> 4 [label=b]
	2 -> "1,123" [label=a]
	"1,123" -> "1,123" [label=a]
}
